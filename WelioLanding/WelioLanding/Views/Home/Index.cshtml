<content id="content">
    <div class="content-body">
        <div class="container">
            <form id="form-register" method="post">
                <div class="row">
                    <div class="col-lg-12 col-md-12 margin-bottom-20">
                        <label class="strong-label">Name*</label>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-6 col-md-12 margin-bottom-20">
                        <label class="small-label">First Name</label>
                        <input type="text" class="input" name="FirstName" id="FirstName"/>
                    </div>
                    <div class="col-lg-6 col-md-12 ">
                        <label class="small-label">Last Name</label>
                        <input type="text" class="input" name="LastName" id="LastName"/>
                    </div>
                </div>

                <div class="row s-label">
                    <div class="col-md-12">
                        <label class="strong-label">Email Address*</label>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <input type="text" class="input" name="Email" id="Email" />
                    </div>
                </div>

                <div class="row s-submit">
                    @*<div class="col-md-3 col-xs-6">
                        <div class="g-recaptcha" id="rcaptcha" data-sitekey="6LeyghsUAAAAABedZa7x0WQqLJDczqvRpHzxIkrA" data-callback="enableBtn" onclick="callbackRC()"></div>
                    </div>*@
                    <div class="col-md-3 col-xs-6 btn-submit">
                        <button class="submit" id="btnsubmit">SUBMIT</button>
                    </div>
                </div>
            </form>
            @*}*@
        </div>
    </div>
    <div class="footer">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <hr />
                </div>

            </div>
            <div class="row">
                <div class="col-md-4 col-xs-4 footer-left-custom">
                    <img src="~/Content/images/logo_greatdebate copy.png">
                </div>
                <div class="col-md-4 col-xs-4 footer-message-custom">
                    <label class="footer-message">© Welio Pty Ltd 2017</label>
                    @*<label class="footer-message">Hello@welio.com</label>*@
                    
                </div>
                <div class="col-md-4 col-xs-4 footer-logo-right-custom">
                    <img src="~/Content/images/footer-logo.png" />
                </div>
            </div>
        </div>
    </div>
</content>


